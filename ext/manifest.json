{
 "background": {"scripts": ["background.js", "lib.js", "googledate.js", "parse_domain.js"]},
 "browser_action": {
 "default_icon": "icon-128.png",
 "default_title": "DigiPo"
 },
"options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
"name": "Digipo",
"description": "Tool support for the DigiPo fact-checking course",
"homepage_url": "http://digipo.io",
"icons": {
     "16": "icon-16.png",
     "48": "icon-48.png",
     "128": "icon-128.png" },
"web_accessible_resources": [
    "/web/viewer.html"
  ],

"permissions": [
     "*://*/*",
     "contextMenus",
	 "activeTab",
     "tabs",
	 "storage"
 ],
"version": "1.1",
"manifest_version": 2
}